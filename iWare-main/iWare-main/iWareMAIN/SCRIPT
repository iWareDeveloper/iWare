local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("                                         ⊙ɪᴡᴀʀᴇ ʙᴇᴛᴀ⊙", "BloodTheme")

-- MAIN
local Main = Window:NewTab("⊙ Main")
local MainSection = Main:NewSection("Thank you for using iWare BETA!")

MainSection:NewKeybind("GUI Toggle", "Sets a keybind to turn the GUI on/off.", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)

-- AIM
local Aim = Window:NewTab("⊙ Aim")
local AimSection = Aim:NewSection("Aim Section - iWare")

AimSection:NewButton("Aimlock ( No Team Check )", "Locks your camera into your opponents head!", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/iWareDeveloper/iWare/main/iWare-main/iWare-main/iWareMISC/AIMLOCK.TC'))()
end)

-- PLAYER
local Player = Window:NewTab("⊙ Player")
local PlayerSection = Player:NewSection("Player Section - iWare")

PlayerSection:NewButton("Fly", "Allows you to fly!", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/iWareDeveloper/iWare/main/iWare-main/iWare-main/iWareMISC/FLY'))()
end)

PlayerSection:NewToggle("WalkSpeed Changer", "Makes you walk so fast! ", function(state)
    if state then
        game.Players.localPlayer.Character.Humanoid.WalkSpeed = 130
    else
        game.Players.localPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

PlayerSection:NewToggle("JumpHeight Changer", "Makes you jump even more higher!", function(state)
if state then
        game.Players.localPlayer.Character.Humanoid.JumpPower = 130
    else
        game.Players.localPlayer.Character.Humanoid.JumpPower = 50
    end
end) 

PlayerSection:NewButton("Wall Walk", "Allows you to walk on walls just like spiders!", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/iWareDeveloper/iWare/main/iWare-main/iWare-main/iWareMISC/WALLWALK'))()
end)

PlayerSection:NewButton("Click Teleport ( CTRL + CLICK )", "Allows you to teleport anywhere u like!", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/iWareDeveloper/iWare/main/iWare-main/iWare-main/iWareMISC/CLICKTP'))()
end)

