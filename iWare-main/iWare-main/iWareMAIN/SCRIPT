local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "iWare | BETA", HidePremium = false,IntroText = "iWare | Discord.io/iWareX", SaveConfig = true, ConfigFolder = "iWareCFG"})

-- MAIN
local Main = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4034483344",
	PremiumOnly = false
})

local Section = Main:AddSection({
	Name = "Thank you for using iWare | BETA!"
})

local Section = Main:AddSection({
	Name = "If you want any help with the script -> Discord.io/iWareX"
})

